import java.util.*;
public class reverse_natural_no {

	public static void main(String[] args)
	{
		int n;
		System.out.println("please enter range to print natural no.in reverse order");
		Scanner sc=new Scanner(System.in);
        n=sc.nextInt();
        for(int i=n;i>=1;i--)
        {
        	System.out.println(i);
        }
	}

}
