import java.util.Scanner;

public class ArrayTotalMarks {

	public static void main(String[] args)
	{
		int mark[]=new int[5];
	    int add=0;
		System.out.println("enter 5 subjects marks");
		Scanner sc=new Scanner(System.in);
		for(int i=0;i<5;i++)
		{
			mark[i]=sc.nextInt();
			add+=mark[i];
        }
		
		System.out.println("total of 5 subjects marks is="+add);
		
		

	}

}

/*
 * output-
 * enter 5 subjects marks
50
50
50
40
60
total of 5 subjects marks is=250
*/
