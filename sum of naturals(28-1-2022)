import java.util.*;
public class sum_natural_no {

	public static void main(String[] args)
	{
		int n,sum=0;
		System.out.println("enter range  ");
		Scanner sc=new Scanner(System.in);
		n=sc.nextInt();
		for(int i=1;i<=n;i++)
		{
			sum=sum+i;
			
			
		}
		System.out.println("sum=  "+sum);

	}

}
/*
output=-
enter range  
5
sum=  15
*/
