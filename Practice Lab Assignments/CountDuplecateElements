public class CountDuplecateElements {

	public static void main(String[] args)
	{
		int[]  array= new int [] {1,2,4,6,3,2,4,1};
		int count=0;
		for(int i=0;i<array.length;i++)
		{
			for(int j=i+1;j<array.length;j++)
			{
				if(array[i]==array[j])
				{
					count++;
					
				}
			}
			
		}
		System.out.println("count of duplecte elements = "+count);

	}

}

/*
output-
count of duplecte elements = 3
*/
